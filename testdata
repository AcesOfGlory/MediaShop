SELECT
  F.filmtitle AS 'Film Title',
  OP.payid AS 'Payment Number'
FROM
  fss_OnlinePayment OP
INNER JOIN
  fss_Person P ON(P.personid = OP.custid)
INNER JOIN
  fss_FilmPurchase FP ON(FP.payid = OP.payid)
INNER JOIN
  fss_Film F ON(F.filmid = FP.filmid)
WHERE
  P.personid = 39
ORDER BY
  `Payment Number` ASC



SELECT
  P.personname AS 'Name',
  OP.payid AS 'Total Payments'
FROM
  fss_OnlinePayment OP
INNER JOIN
  fss_Person P ON(P.personid = OP.custid)
WHERE
  P.personid = '39'


melissa.dejesus@home.co.uk


UPDATE
  fss_Address A
SET
  A.addstreet = "22 Quis Rd.",
  A.addcity = "Huddersfield",
  A.addpostcode = "HD2 3AS"
INNER JOIN
  fss_CustomerAddress CA ON(A.addid = CA.addid)
WHERE
  CA.custid = 39

  UPDATE
    A
  SET
    A.addstreet = "22 Quis Rd."
  FROM
    fss_Address AS A
  INNER JOIN
    fss_CustomerAddress AS CA ON(A.addid = CA.addid)
  WHERE
    CA.custid = 39;

